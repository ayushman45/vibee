<%-include("partials/header")%>
<%-include("partials/nav")%>

<form method="POST"action="/signup" autocomplete="off">
    <div class="row mb-3">
      <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
      <div class="col-sm-10">
        <input type="email" name="email" class="form-control" id="inputEmail3">
      </div>
    </div>
    <div class="row mb-3">
        <label for="firstname" class="col-sm-2 col-form-label">First Name</label>
        <div class="col-sm-10">
          <input type="text" name="fname" class="form-control" id="firstname">
        </div>
      </div>
      <div class="row mb-3">
        <label for="lastname" class="col-sm-2 col-form-label">Last Name</label>
        <div class="col-sm-10">
          <input type="text" name="lname" class="form-control" id="lastname">
        </div>
      </div>
      <div class="row mb-3">
        <label for="uname" class="col-sm-2 col-form-label">Username</label>
        <div class="col-sm-10">
          <input type="text" name="username" class="form-control" id="uname">
        </div>
      </div>
    <div class="row mb-3">
      <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
      <div class="col-sm-10">
        <input type="password" name="password" class="form-control" id="inputPassword3">
      </div>
    </div>
    <button type="submit" class="btn btn-primary">Sign in</button>
  </form>

<%-include("partials/footer")%>